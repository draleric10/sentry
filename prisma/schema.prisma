generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Master_Client {
  id String @id @default(dbgenerated()) @map("_id") @db.ObjectId
}

model Master_User {
  id            String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  active_yn     String
  clientid_no   String
  email_address String
  first_name    String
  groupid_no    String
  img_tx        String
  last_name     String
}
